package com.kh.ch05_object;

public class Studer {
	
	//필드부 : [접근제환자] [예약어] 자료형 필드명;
		private String name;	//이름
		private int age;		//나이
		private	double height;	//키
		private String phone; 	//연락처
		private String address;	//주소
		
		
	//생성자부 : 접근제환자 클래스명([매개변수]){}
		public Studer(String name, int age, double height, String phone, String address) {
			this.name=name;
			this.age=age;
			this.height=height;
			this.phone=phone;
			this.address=address;
		}
		
		
	//메소드부	 : [접근제환자] [예약어] 반환타입 메소드명([매개변수]){}
		public static void ("name, age, height, phone, address") {}
		
}
