package com.kh.ch14_collection;

import java.util.HashMap;

public class MapRun {

	
		/*
		 * Map<K,V> : key-value 형식으로 데이터를 관리하는 콜랙션
		 * => List, Set 과는 달리 Collection을 구현한 클래스는 아님
		 * 	(메소드가 일부 다름)
		 */
	public static void main(String[] args) {
		HashMap<String. Snack> hMap = new HashMap<>();
		
		
		hMap.put("바나나킥", new Snack("바나나맛", 1024));
		hMap.put("도리토스", new Snack("바베큐맛", 1024));
		hMap.put("프링글스", new Snack("바나나맛", 1024));
	}
	
	
	}
	
	Class Snack{
		private String flavor;
		private int calory;
		
		public Snack(String flavor, int calory) {
			this.flavor=flavor;
			this.calory
		}

}
